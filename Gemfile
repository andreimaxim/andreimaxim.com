source "https://rubygems.org"

gem "jekyll", "~> 4.3"

# Jekyll plugins that need to be set up via _config.yml
# in order to be loaded.
gem "jekyll-paginate", "~> 1.1"
gem "jekyll-sitemap", "~> 1.4"
gem "jekyll-feed", "~> 0.17"

# Jekyll plugins that are loaded regardless of the site
# configuration.
group :jekyll_plugins do
  gem "jekyll-compose", "~> 0.12"
end

gem "webrick"
